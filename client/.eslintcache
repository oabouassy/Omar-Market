ned] = useState(false);\n  const { loggedUser, setLoggedUser } = useContext(loginContext);\n  // 2 states for the form input values\n  const [email, setEmail] = useState(\"\");\n  const [password, setPassword] = useState(\"\");\n\n  // history object to direct user to another router after signing in\n  const history = useHistory();\n\n  // a function to deal with posting/getting data to/from  client\n  const handleSubmit = async (e) => {\n    e.preventDefault();\n    e.target.reset();\n    let info = {\n      email,\n      password,\n    };\n\n    // send data to the server\n    try {\n      const res = await fetch(\n        \"https://omar-market-api.herokuapp.com/api/signin\",\n        {\n          method: \"POST\",\n          headers: { \"Content-type\": \"application/json\" },\n          body: JSON.stringify(info),\n        }\n      );\n      const data = await res.json();\n      if (data.message) {\n        setError(data.message);\n      } else {\n        setLoggedUser(data);\n        setIsSigned(true);\n      }\n      if (data.isadmin) {\n        setTimeout(() => {\n          history.push(\"/dashboard\");\n        }, 2000);\n      } else {\n        setTimeout(() => {\n          history.push(\"/\");\n        }, 2000);\n      }\n    } catch (err) {\n      console.log(err);\n    }\n  };\n  const handleSignOut = async () => {\n    try {\n      const res = await fetch(\n        \"https://omar-market-api.herokuapp.com/api/logout\"\n      );\n      setLoggedUser({});\n    } catch (err) {\n      console.log(err);\n    }\n  };\n\n  return (\n    <div className=\"signin\">\n      {loggedUser.email ? (\n        <div className=\"signed_in\">\n          <h1 className=\"signin_welcome\">\n            Hi {loggedUser.firstname}, You are logged in\n          </h1>\n          {isSigned && loggedUser.isadmin ? (\n            <h1 className=\"signin_redirectionMessage\">\n              Welcome back boss! You will be redirected to your admin dashboard\n              ...\n            </h1>\n          ) : null}\n          {isSigned && !loggedUser.isadmin ? (\n            <h1 className=\"signin_redirectionMessage\">\n              Glad to see you again {loggedUser.firstname}, you will be\n              redirected to the home page ...\n            </h1>\n          ) : null}\n          <button className=\"SignIn_signout_btn\" onClick={handleSignOut}>\n            Sign Out\n          </button>\n        </div>\n      ) : (\n        <div className=\"not_signin\">\n          <h1 className=\"not_signin_error\">{error}</h1>\n          <div className=\"container\">\n            <form className=\"signin_form\" onSubmit={handleSubmit}>\n              <h1 className=\"signin_heading\">Log in to your account</h1>\n              <div className=\"not_signin_fields\">\n                <input\n                  type=\"email\"\n                  name=\"email\"\n                  placeholder=\"type your email\"\n                  value={email}\n                  onChange={(e) => setEmail(e.target.value)}\n                  disabled={loggedUser.email}\n                />\n                <input\n                  type=\"password\"\n                  name=\"password\"\n                  placeholder=\"type your password\"\n                  value={password}\n                  onChange={(e) => setPassword(e.target.value)}\n                  disabled={loggedUser.email}\n                />\n                <button type=\"submit\" className=\"signin_btn\">\n                  Sign in\n                </button>\n              </div>\n            </form>\n          </div>\n        </div>\n      )}\n    </div>\n  );\n};\nexport default SignIn;\n","/home/omar/DATA/Programming/Projects/omar-market/client/src/components/SignUp.js",[],"/home/omar/DATA/Programming/Projects/omar-market/client/src/components/About.js",[],"/home/omar/DATA/Programming/Projects/omar-market/client/src/components/Home.js",[],"/home/omar/DATA/Programming/Projects/omar-market/client/src/components/Products.js",[],"/home/omar/DATA/Programming/Projects/omar-market/client/src/components/Dashboard.js",[],"/home/omar/DATA/Programming/Projects/omar-market/client/src/components/SingleProduct.js",["87"],"import loginContext from \"../contexts/loginContext\";\nimport { useEffect, useState, useContext } from \"react\";\nimport \"../styles/SingleProduct.css\";\nimport { useHistory } from \"react-router-dom\";\nimport { Link } from \"react-router-dom\";\nconst SingleProduct = ({ match }) => {\n  const { loggedUser } = useContext(loginContext);\n  const [item, setItem] = useState({});\n  const history = useHistory();\n  const id = match.params.id;\n  useEffect(() => {\n    const fetchProduct = async () => {\n      const res = await fetch(\n        `https://omar-market-api.herokuapp.com/api/product/${id}`\n      );\n      const data = await res.json();\n      setItem(data);\n    };\n    fetchProduct();\n  }, []);\n  const deletePost = async (e) => {\n    // delete post\n    const res = await fetch(\n      `https://omar-market-api.herokuapp.com/api/product/${id}`,\n      { method: \"DELETE\" }\n    );\n    const data = await res.json();\n    if (data.message) {\n      history.push(\"/products\");\n    }\n  };\n  return (\n    <div className=\"singleProduct\">\n      <div className=\"container\">\n        <h1 className=\"singleProduct_name\">{item.name}</h1>\n        <img\n          className=\"singleProduct_image\"\n          src={item.thumbnail_url}\n          alt=\"product\"\n        />\n        <p className=\"singleProduct_about\">{item.about}</p>\n        <div className=\"singleProduct_rating_category_container\">\n          <div className=\"singleProduct_category\">{item.category}</div>\n          <div className=\"singleProduct_rating\">\n            <i className=\"ico fas fa-star\"></i>\n            <span className=\"actual_rating\">{item.rating}</span>\n          </div>\n        </div>\n        {loggedUser.isadmin ? (\n          <button onClick={deletePost} className=\"singleProduct_delete\">\n            Delete this product\n          </button>\n        ) : (\n          <Link to=\"/sign-in\">\n            <button className=\"singleProduct_delete\">\n              You are admin ? Sign In\n            </button>\n          </Link>\n        )}\n      </div>\n    </div>\n  );\n};\nexport default SingleProduct;\n","/home/omar/DATA/Programming/Projects/omar-market/client/src/components/NotFound.js",[],"/home/omar/DATA/Programming/Projects/omar-market/client/src/components/Nav.js",["88"],"import \"../styles/Nav.css\";\nimport { Link } from \"react-router-dom\";\nimport loginContext from \"../contexts/loginContext\";\nimport { useContext, useEffect, useState } from \"react\";\nimport ListItem from \"./ListItem\";\nconst Nav = () => {\n  const [isOpen, setIsOpen] = useState(false);\n  const { loggedUser } = useContext(loginContext);\n  const toggleMenu = () => {\n    setIsOpen(!isOpen);\n  };\n\n  return (\n    <nav className=\"Nav\">\n      <div className=\"container\">\n        <div className=\"nav_outer_container\">\n          <div className=\"nav_main_container\">\n            <h1 className=\"logo\">\n              <Link to=\"/\">Omar Market</Link>\n            </h1>\n            <i className=\"ico fas fa-bars\" onClick={toggleMenu}></i>\n          </div>\n          <ul className={isOpen ? \"list open\" : \"list\"}>\n            <ListItem to=\"/\" name=\"Home\" />\n            <ListItem to=\"/products\" name=\"products\" />\n            <ListItem to=\"/about\" name=\"about\" />\n\n            {loggedUser.isadmin ? (\n              <ListItem to=\"/dashboard\" name=\"dashboard\" />\n            ) : null}\n\n            <ListItem to=\"/sign-up\" name=\"Sign Up\" />\n            <ListItem\n              to=\"/sign-in\"\n              name={\n                loggedUser.email ? `${loggedUser.firstname} account` : \"Sign In\"\n              }\n            />\n          </ul>\n        </div>\n      </div>\n    </nav>\n  );\n};\nexport default Nav;\n